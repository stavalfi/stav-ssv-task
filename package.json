  "scripts": {
    "docker-compose:up": "docker-compose up --remove-orphans",
    "docker-compose:down": "docker-compose down --remove-orphans",
    "docker:build": "docker build . -t stavalfi/ssv-task:1.0.0",
    "docker:push": "docker push stavalfi/ssv-task:1.0.0",
    "start:production": "node dist/index.js",
    "start:dev": "ts-node --swc src/index.ts",
    "start:dev:watch": "nodemon",
    "build": "tsc",
    "clean": "rm -rf deployments/localhost cache artifacts typechain-types",
    "prettier:all": "yarn prettier --write .",
    "postinstall": "husky install",
    "lint:code:base": "eslint --max-warnings 0",
    "lint:code": "yarn lint:code:base '**/*.{ts,d.ts,tsx}'"
