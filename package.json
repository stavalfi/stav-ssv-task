    "eslint": "8.19.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-plugin-clean-regex": "^0.5.1",
    "eslint-plugin-functional": "4.2.0",
